{
  "include": ["src/**/*"],
  "compilerOptions": {
    "target": "es2018",
    "module": "commonjs",
    "lib": ["esnext.asynciterable", "es2018", "dom"],
    "outDir": "dist",
    "strict": true,
    "strictPropertyInitialization": false,
    "esModuleInterop": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "forceConsistentCasingInFileNames": true
  }
}
